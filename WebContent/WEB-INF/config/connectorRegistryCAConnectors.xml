<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--***********************************************************

    ConnectorRegistry for Controlled Availability Connectors.

    As per this concept for the list of connectors we need to
    ensure that in 6.4 onwards the Application Template is not
    imported by default.

    There is an attribute "CAConnector" on each of these
    types that will filter them from the Application UI.

    This file is imported by the system only in case of install
    and upgrade.

*************************************************************-->
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>

  <ImportAction name="mergeConnectorRegistry">
    <Configuration>
      <Attributes>
        <Map>
          <entry key='fileList'>
            <value>
              <List>
                <String>WEB-INF/config/connector/BEAAqualogicEnterpriseSecurity.xml</String>
                <String>WEB-INF/config/connector/Cerner.xml</String>
                <String>WEB-INF/config/connector/EPIC.xml</String>
                <String>WEB-INF/config/connector/GECentricity.xml</String>
                <String>WEB-INF/config/connector/IBMi.xml</String>
                <String>WEB-INF/config/connector/Workday.xml</String>
              </List>
            </value>
          </entry>
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>
</sailpoint>
