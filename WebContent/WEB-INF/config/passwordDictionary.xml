<!--
  Initialization file for password dictionary
-->
<sailpoint>

  <!--
    Password Dictionary
   -->
   <ImportAction name='merge'>
     <Dictionary name="PasswordDictionary">
      <Terms>
        <DictionaryTerm value="password"/>
        <DictionaryTerm value="identity"/>
      </Terms>
     </Dictionary>
   </ImportAction>
</sailpoint>