<?xml version='1.0' encoding='UTF-8'?>
<ImportAction name="connectorRegistryUpdateAction">
    <Application connector="sailpoint.connector.OpenConnectorAdapter" featuresString="PROVISIONING,SEARCH,MANAGER_LOOKUP,DISCOVER_SCHEMA" icon="enterpriseIcon" name="PeopleSoftHRMS-Direct" type="PeopleSoftHRMS-Direct">
        <Attributes>
            <Map>
                <entry key="connectorClass" value="openconnector.connector.PeopleSoftHRMSConnector" />
                <entry key="formPath" value="PeopleSoftHrmsAttributesForm.xhtml" />
                <entry key="encrypted" value="domainConnPassword" />
            </Map>
        </Attributes>
        <Schemas>
            <Schema displayAttribute="KEYPROP_EMPLID" identityAttribute="KEYPROP_EMPLID" objectType="account">
                <AttributeDefinition name="KEYPROP_EMPLID" type="string">
                    <Description>ID of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_BIRTHCOUNTRY" type="string">
                    <Description>Birth country of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_BIRTHPLACE" type="string">
                    <Description>Birth place of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_BIRTHSTATE" type="string">
                    <Description>Birth state of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_DT_OF_DEATH" type="string">
                    <Description>Date when the employee passed away</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_DERIVED_CWR" type="string">
                    <Description>Contingent worker</Description>
                </AttributeDefinition>
                <AttributeDefinition name="PROP_PER_ORG" type="string">
                    <Description>Organization of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_CONTRACT_DATA" type="string">
                    <Description>Employee details of the contract data</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_EMAIL_ADDRESSES" type="string">
                    <Description>Employee details of the email data</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_ADDRESS_TYPE_VW" type="string">
                    <Description>Employee address details</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_NAME_TYPE_VW" type="string">
                    <Description>Employee details such as first name,last name and so on.</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_PERSONAL_PHONE" type="string">
                    <Description>Employee phone details</Description>
                </AttributeDefinition>
                <AttributeDefinition name="COLL_JOB" type="string">
                    <Description>Job details of the employee</Description>
                </AttributeDefinition>
                <AttributeDefinition name="BUSINESS_EMAIL" type="string">
                    <Description>Employee email of business type</Description>
                </AttributeDefinition>
                <AttributeDefinition name="BUSINESS_PHONE" type="string">
                    <Description>Employee phone number of business type</Description>
                </AttributeDefinition>
            </Schema>
        </Schemas>
    </Application>
</ImportAction>
