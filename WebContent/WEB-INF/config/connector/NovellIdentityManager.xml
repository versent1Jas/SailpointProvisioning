    <Attributes>
      <Map>
        <entry key="3.0">
          <value>
            <Map>
              <entry key="Description" value="Description"/>
              <entry key="LastName" value="LastName"/>
              <entry key="cN" value="cN"/>
              <entry key="dirXML-Associations" value="dirXML-Associations"/>
