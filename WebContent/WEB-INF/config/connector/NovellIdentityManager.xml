        <AttributeDefinition name="srvprvHideAttributes" type="string"/>
        <AttributeDefinition name="LastName" type="string"/>
        <AttributeDefinition name="loginDisabled" type="boolean"/>
        <AttributeDefinition name="manager" type="string"/>
